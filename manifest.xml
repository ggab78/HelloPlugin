<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin certificationLevel="None" displayName="Hello Plugin" minSystemVersion="7.1" name="helloPlugin" version="1.0">
  <Attributes>
    <Map>
      <entry key="minUpgradableVersion" value="1.0" />
      <entry key="fullPage">
        <value>
          <FullPage title="Hello" />
        </value>
      </entry>
      <entry key="snippets">
        <value>
          <List>
            <Snippet regexPattern=".*" rightRequired="ViewTodoPluginIcon">
              <Scripts>
                <String>ui/js/snippets/header.js</String>
              </Scripts>
              <StyleSheets>
                <String>ui/css/hello.css</String>
              </StyleSheets>
            </Snippet>
          </List>
        </value>
      </entry>
    </Map>
  </Attributes>
</Plugin>
